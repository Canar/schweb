#!/usr/bin/env -S csi -s

(import srfi-1 srfi-13 matchable format (chicken process-context))

(define (format-local str . args)
  (apply format str args))

(define (arguments) (command-line-arguments))

(include "util-string-replace-all-std.ss")

(include "html.ss")

